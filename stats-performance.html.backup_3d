<script>
// ATTENDRE QUE TOUT SOIT CHARG√â Y COMPRIS LES ORBIT RINGS
function applyHoverEffects() {
    // TROUVER LA GRID DES 4 CARTES
    const grid = document.querySelector('div[style*="display: grid"][style*="repeat(4, 1fr)"]');
    
    if (!grid) {
        console.warn('‚è≥ Grid non trouv√©e, retry dans 500ms...');
        setTimeout(applyHoverEffects, 500);
        return;
    }
    
    // TROUVER LES 4 CARTES DEDANS
    const cards = grid.querySelectorAll('div[style*="background: rgba(15, 20, 30"]');
    
    if (cards.length === 0) {
        console.warn('‚è≥ Cartes non trouv√©es, retry dans 500ms...');
        setTimeout(applyHoverEffects, 500);
        return;
    }
    
    console.log('üéØ Cartes Orbit Rings trouv√©es:', cards.length);
    
    cards.forEach((card, i) => {
        // FORCER LA TRANSITION
        card.style.transition = 'all 0.3s cubic-bezier(0.4, 0, 0.2, 1)';
        card.style.cursor = 'pointer';
        
        console.log(`‚úÖ Carte ${i+1} (SEMAINE ${i+1}) configur√©e`);
        
        // AJOUTER LES √âV√âNEMENTS HOVER
        card.addEventListener('mouseenter', function() {
            this.style.transform = 'translateY(-5px)';
            this.style.border = '2px solid #00e5ff';
            this.style.boxShadow = '0 8px 25px rgba(0, 229, 255, 0.4)';
            this.style.background = 'rgba(0, 229, 255, 0.15)';
        });
        
        card.addEventListener('mouseleave', function() {
            this.style.transform = 'translateY(0)';
            this.style.border = '2px solid rgba(0, 229, 255, 0.4)';
            this.style.boxShadow = 'none';
            this.style.background = 'rgba(15, 20, 30, 0.95)';
        });
        
        card.addEventListener('mousedown', function() {
            this.style.transform = 'translateY(-2px) scale(0.98)';
        });
        
        card.addEventListener('mouseup', function() {
            this.style.transform = 'translateY(-5px) scale(1)';
        });
    });
    
    console.log('‚úÖ Effets hover appliqu√©s sur toutes les cartes !');
}

// ATTENDRE QUE TOUT SOIT CHARG√â
window.addEventListener('load', () => {
    setTimeout(applyHoverEffects, 1000);
});

// AUSSI ESSAYER AU DOMContentLoaded
document.addEventListener('DOMContentLoaded', () => {
    setTimeout(applyHoverEffects, 1000);
});
</script>
